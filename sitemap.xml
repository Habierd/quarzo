<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% assign pages = site.pages | where_exp: "p", "p.sitemap != false" %}
  {% for p in pages %}
  <url><loc>{{ site.url }}{{ p.url }}</loc></url>
  {% endfor %}
  {% for p in site.products %}
  <url><loc>{{ site.url }}{{ p.url }}</loc></url>
  {% endfor %}
</urlset>
